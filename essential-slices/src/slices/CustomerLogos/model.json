	"icon": "person",
	"display": "list",
	"non-repeat": {
		"eyebrow_headline": {
			"type": "StructuredText",
			"config": {
				"single": "heading2",
				"label": "Eyebrow Headline",
				"placeholder": "Trusted by"
			}
		},
		"call_to_action": {
			"type": "StructuredText",
			"config": {
				"single": "paragraph",
				"label": "Call To Action",
