			}
		}
	},
	"repeat": {
		"logo": {
			"type": "Image",
			"config": {
				"constraint": {},
				"thumbnails": [],
				"label": "Logo"
			}
		},
		"link": {
			"type": "Link",
			"config": {
				"allowTargetBlank": true,
				"label": "link",
				"placeholder": "Could be a link to use case, press article, signup..."
			}
		}
	}
