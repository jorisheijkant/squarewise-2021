      "items": {
        "client_logo": {
          "type": "Image",
          "config": {
            "constraint": {
              "width": 100,
              "height": 100
            },
            "thumbnails": [],
            "label": "Client logo"
          }
        },
        "profile_pic": {
          "type": "Image",
          "config": {
            "constraint": {
              "width": 80,
              "height": 80
            },
            "thumbnails": [],
            "label": "Profile Pic"
          }
        },
        "quote": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph",
